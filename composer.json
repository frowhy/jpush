{
    "name": "frowhy/jpush",
    "description": "Laravel JPush",
    "type": "library",
    "homepage": "https://github.com/frowhy/jpush",
    "license": "MIT",
    "authors": [
        {
            "name": "付广辉",
            "email": "frowhy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "jpush/jpush": "^3.5",
        "illuminate/support": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Frowhy\\JPush\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "JPushFactory": "Frowhy\\JPush\\Facades\\JPushFactory"
            },
            "providers": [
                "Frowhy\\JPush\\Providers\\LaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
